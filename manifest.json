{
	"name": "RPI Web Improvements",
	"short_name": "Sisyphus",
	"description": "Make SIS a little less ugly.",
	"author": "Kai",
	"version": "0.1.1",
	"icons": {
		"128": "logo128.png"
	},
	"content_scripts": [
		{
			"css": [
				"defines.css",
				"sspfont.css",
				"app.css",
				"header.css",
				"footer.css",
				"info.css",
				"weekly_schedule.css",
				"site_map.css",
				"emergency_contacts.css"
			],
			"matches": [
				"https://sis.rpi.edu/*"
			]
		}
	],
	"web_accessible_resources": [
		{
			"resources": [
				"page_assets/*"
			],
			"matches": [
				"https://sis.rpi.edu/*"
			]
		}
	],
	"manifest_version": 3
}